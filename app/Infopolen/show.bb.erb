      <tr>
    	<td class="itemLabel">Semana</td>
        <td class="itemValue"><%= @infopolen.semana%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Estaciones</td>
        <td class="itemValue"><%= @infopolen.estaciones%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">TiposPolinicos</td>
        <td class="itemValue"><%= @infopolen.tipos_polinicos%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Precedentes</td>
        <td class="itemValue"><%= @infopolen.precedentes%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Prevision</td>
        <td class="itemValue"><%= @infopolen.prevision%></td>
      </tr>
      
    </table>
  </div>

</div>
