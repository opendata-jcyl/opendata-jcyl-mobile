    <%= link_to "Edit", :action => :edit, :id => @infopolen.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Id</td>
        <td class="itemValue"><%= @infopolen.id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Fecha</td>
        <td class="itemValue"><%= @infopolen.fecha%></td>
      </tr>
      
      <tr>
