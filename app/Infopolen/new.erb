<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @infopolen.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @infopolen.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="infopolen[id]" class="fieldLabel">Id</label>
            <input type="text" id="infopolen[id]" name="infopolen[id]" <%= placeholder("Id") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="infopolen[fecha]" class="fieldLabel">Fecha</label>
            <input type="text" id="infopolen[fecha]" name="infopolen[fecha]" <%= placeholder("Fecha") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="infopolen[anio]" class="fieldLabel">Anio</label>
            <input type="text" id="infopolen[anio]" name="infopolen[anio]" <%= placeholder("Anio") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="infopolen[semana]" class="fieldLabel">Semana</label>
            <input type="text" id="infopolen[semana]" name="infopolen[semana]" <%= placeholder("Semana") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="infopolen[estaciones]" class="fieldLabel">Estaciones</label>
            <input type="text" id="infopolen[estaciones]" name="infopolen[estaciones]" <%= placeholder("Estaciones") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="infopolen[tipos_polinicos]" class="fieldLabel">Tipos polinicos</label>
            <input type="text" id="infopolen[tipos_polinicos]" name="infopolen[tipos_polinicos]" <%= placeholder("Tipos polinicos") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="infopolen[precedentes]" class="fieldLabel">Precedentes</label>
            <input type="text" id="infopolen[precedentes]" name="infopolen[precedentes]" <%= placeholder("Precedentes") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="infopolen[prevision]" class="fieldLabel">Prevision</label>
            <input type="text" id="infopolen[prevision]" name="infopolen[prevision]" <%= placeholder("Prevision") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
